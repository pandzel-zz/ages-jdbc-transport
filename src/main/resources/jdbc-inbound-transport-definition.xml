<transport name="JDBC" label="${com.esri.geoevent.transport.jdbc-transport.JDBC_LABEL}" contact="yourname@yourcompany.com" version="${project.version}" domain="sample.transport.inbound" type="inbound">
	<description>${com.esri.geoevent.transport.jdbc-transport.JDBC_DESC}</description>
	<propertyDefinitions>
		<propertyDefinition propertyName="connectionUrl" label="${com.esri.geoevent.transport.jdbc-transport.JDBC_URL_LABEL}"
			description="${com.esri.geoevent.transport.jdbc-transport.JDBC_URL_DESC}"
			propertyType="String" mandatory="true" readOnly="false">
		</propertyDefinition>
		<propertyDefinition propertyName="userName" label="${com.esri.geoevent.transport.jdbc-transport.JDBC_USERNAME_LABEL}"
			description="${com.esri.geoevent.transport.jdbc-transport.JDBC_USERNAME_DESC}"
			propertyType="String" mandatory="true" readOnly="false">
		</propertyDefinition>
		<propertyDefinition propertyName="password" label="${com.esri.geoevent.transport.jdbc-transport.JDBC_PASSWORD_LABEL}"
			description="${com.esri.geoevent.transport.jdbc-transport.JDBC_PASSWORD_DESC}"
			propertyType="Password" mandatory="true" readOnly="false">
		</propertyDefinition>
		<propertyDefinition propertyName="eventRate" label="${com.esri.geoevent.transport.jdbc-transport.JDBC_EVENT_RATE_LBL}"
			description="${com.esri.geoevent.transport.jdbc-transport.JDBC_EVENT_RATE_DESC}"
			propertyType="Integer" defaultValue="1" mandatory="true" readOnly="false">
		</propertyDefinition>
	</propertyDefinitions>
</transport>
